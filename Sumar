#include <iostream>

using namespace std;

int main() {
    int arreglo[5];
    int suma = 0;

    cout << "Ingrese 5 numeros enteros:" << endl;

    for (int i = 0; i < 5; i++) {
        cout << "Elemento " << i + 1 << ": ";
        cin >> arreglo[i];
    }

    for (int i = 0; i < 5; i++) {
        suma += arreglo[i];
    }

    cout << "\nLa suma total es: " << suma << endl;

    return 0;
}
